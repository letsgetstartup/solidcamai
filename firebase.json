{
    "functions": [
        {
            "source": "functions",
            "runtime": "python311",
            "codebase": "default",
            "ignore": [
                "venv",
                ".git",
                "firebase-debug.log",
                "firebase-debug.*.log"
            ]
        }
    ],
    "hosting": [
        {
            "public": "web",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "rewrites": [
                {
                    "source": "/dashboard",
                    "destination": "/dashboard.html"
                },
                {
                    "source": "/bigscreen",
                    "destination": "/bigscreen.html"
                },
                {
                    "source": "/equations{,/**}",
                    "destination": "/equations/index.html"
                },
                {
                    "source": "/pair_init",
                    "function": "pair_init"
                },
                {
                    "source": "/pair_token",
                    "function": "pair_token"
                },
                {
                    "source": "/equations_api/**",
                    "function": "equations_api"
                },
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ],
            "target": "prod"
        },
        {
            "target": "simco",
            "public": "web_simco_public",
            "ignore": [
                "firebase.json",
                "**/.*",
                "**/node_modules/**"
            ],
            "rewrites": [
                {
                    "source": "/ask",
                    "function": "ask"
                },
                {
                    "source": "**",
                    "destination": "/index.html"
                }
            ]
        }
    ]
}